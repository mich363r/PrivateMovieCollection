<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="rootPane2" prefHeight="483.0" prefWidth="692.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="privatemoviecollection.GUI.Controller.FXMLDocumentController">
    <children>
        <Button fx:id="btnSearch" layoutX="430.0" layoutY="14.0" onAction="#handleSearch" text="Search" />
      <TableView fx:id="tbViewMovie" layoutX="26.0" layoutY="129.0" prefHeight="215.0" prefWidth="321.0">
        <columns>
          <TableColumn fx:id="colImdbRating" prefWidth="75.0" text="Imdb" />
          <TableColumn fx:id="colPersonalRating" prefWidth="75.0" text="Personal" />
            <TableColumn fx:id="colTitle" prefWidth="170.0" text="Title" />
        </columns>
      </TableView>
      <TableView fx:id="tbViewCategory" layoutX="411.0" layoutY="129.0" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="colCat" prefWidth="199.0" text="C1" />
        </columns>
      </TableView>
      <Label layoutX="26.0" layoutY="55.0" text="Private Movie Collection">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <TextField fx:id="txtSearch" layoutX="511.0" layoutY="14.0" />
      <Button layoutX="605.0" layoutY="426.0" mnemonicParsing="false" onAction="#chooseFile" text="open file" />
      <Button fx:id="btnAddMovie" layoutX="33.0" layoutY="426.0" mnemonicParsing="false" onAction="#addMovie" text="add movie" />
    </children>
</AnchorPane>
